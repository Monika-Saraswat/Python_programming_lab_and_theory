def armstrong(lb,ub):
    for i in range (lb,(ub+1)):
                    l= len(str(i))
                    m=i
                    s=0
                    while m!=0:
                        r=m%10
                        m=m//10
                        s+=r**l
                    if s==i:
                        print(i)
lb=int(input("lb:"))
ub=int(input("ub:"))
armstrong(lb,ub)
